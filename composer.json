{
    "name": "nbpalomino/silex-skeleton",
    "description": "A pre-configured skeleton for the Silex microframework curated by Nick B. Palomino",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.3.3",
        "silex/silex": "~1.0",        
        "doctrine/dbal": "2.*",
        "symfony/config": "~2.3",
        "symfony/console": "~2.3",        
        "symfony/finder": "~2.3",
        "symfony/monolog-bridge":"~2.3",
        "symfony/translation":"~2.3",
        "symfony/form": "~2.3",
        "symfony/process": "~2.3",
        "symfony/security": "~2.3",
        "symfony/twig-bridge": "~2.3",
        "symfony/validator": "~2.3"        
    },
    "require-dev": {
        "symfony/var-dumper": "~2.3",
        "symfony/debug": "~2.3",
        "symfony/browser-kit": "~2.3",
        "symfony/css-selector": "~2.3",
        "symfony/dom-crawler": "~2.3",
        "silex/web-profiler": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Quipulabz\\": "src/Quipulabz"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
